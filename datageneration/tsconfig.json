{
  "compilerOptions": {
    "target": "ESNEXT",
    "module": "commonjs",
    "lib": ["es6","dom"],
    "outDir": "lib",
    "rootDir": "src",
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
